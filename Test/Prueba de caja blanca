 */

// 1. Validación de campos vacíos
function validarCampos(correo, password) {
  if (!correo && !password) {
    return "Ingresa tu correo y contraseña.";
  } else if (!correo) {
    return "Completa tu correo electrónico.";
  } else if (!password) {
    return "Completa tu contraseña.";
  } else {
    return "Inicio de sesión exitoso.";
  }
}

console.log("Prueba 1:", validarCampos("", ""));
console.log("Prueba 2:", validarCampos("tecnico@app.com", "")); 
console.log("Prueba 3:", validarCampos("", "12345"));
console.log("Prueba 4:", validarCampos("usuario@app.com", "12345"));

